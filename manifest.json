{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "二维码生成器",
    "description": "把当前url路径生成二维码，方便移动端调试",
    "icons": {
        "128": "images/icon_128.png",
        "32": "images/icon_32.png"
    },
    "browser_action": {
        "default_icon": "images/icon_32.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}